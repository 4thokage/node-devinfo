{
  "name": "node-devinfo",
  "version": "1.0.0",
  "description": "A blazing fast REST APIs with Node.js, MongoDB, Fastify and Swagger.",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "test": "yarn clean && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "deploy": "exit 0",
    "lint": "eslint"
  },
  "author": "Jose Rodrigues <jsr@jose-rodrigues.infp> (www.jose-rodrigues.info)",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "^6.59.0",
    "@tsed/common": "^6.59.0",
    "@tsed/core": "^6.59.0",
    "@tsed/di": "^6.59.0",
    "@tsed/platform-express": "^6.59.0",
    "@tsed/exceptions": "^6.59.0",
    "@tsed/mongoose": "^6.59.0",
    "@tsed/swagger": "^6.59.0",
    "@tsed/testing-mongoose": "^6.59.0",
    "@types/swagger-schema-official": "^2.0.21",
    "ajv": "^8.6.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "method-override": "^3.0.0",
    "mongoose": "^5.12.14",
    "serve-static": "^1.13.1",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.19",
    "@types/chai-as-promised": "^7.1.4",
    "@types/express": "^4.17.12",
    "@types/http-proxy": "^1.17.1",
    "@types/mocha": "8.0.3",
    "@types/mongoose": "^5.11.97",
    "@types/node": "14.14.34",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "9.0.8",
    "@types/sinon-chai": "3.2.4",
    "@types/supertest": "2.0.10",
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "5.3.0",
    "eslint": "^7.29.0",
    "mocha": "8.2.1",
    "nodemon": "^2.0.7",
    "nyc": "15.1.0",
    "rimraf": "^3.0.0",
    "sinon": "9.0.3",
    "sinon-chai": "3.6.0",
    "supertest": "6.0.0",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4",
    "yarn": "^1.22.10"
  }
}
