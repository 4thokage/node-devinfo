{
  "name": "node-devinfo",
  "version": "1.0.0",
  "description": "A blazing fast REST APIs with Node.js, MongoDB, Fastify and Swagger.",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "test": "yarn clean && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "deploy": "exit 0",
    "lint": "eslint"
  },
  "author": "Jose Rodrigues <jsr@jose-rodrigues.infp> (www.jose-rodrigues.info)",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "^6.73.9",
    "@tsed/common": "^6.73.9",
    "@tsed/core": "^6.73.9",
    "@tsed/di": "^6.73.9",
    "@tsed/platform-express": "^6.73.9",
    "@tsed/exceptions": "^6.73.9",
    "@tsed/mongoose": "^6.73.9",
    "@tsed/swagger": "^6.73.9",
    "@tsed/testing-mongoose": "^6.73.9",
    "@types/swagger-schema-official": "^2.0.22",
    "ajv": "^8.6.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "method-override": "^3.0.0",
    "mongoose": "^6.0.10",
    "serve-static": "^1.14.1",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/express": "^4.17.13",
    "@types/http-proxy": "^1.17.7",
    "@types/mocha": "9.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "16.10.5",
    "@types/request-promise": "^4.1.48",
    "@types/sinon": "10.0.4",
    "@types/sinon-chai": "3.2.5",
    "@types/supertest": "2.0.11",
    "chai": "4.3.4",
    "chai-as-promised": "^7.1.1",
    "concurrently": "6.3.0",
    "eslint": "^8.0.0",
    "mocha": "9.1.2",
    "nodemon": "^2.0.13",
    "nyc": "15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "supertest": "6.1.6",
    "ts-node": "^10.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4",
    "yarn": "^1.22.15"
  }
}
